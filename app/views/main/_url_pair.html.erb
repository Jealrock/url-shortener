<tr>
  <td><%= link_to url_pair.original, url_pair.original, target: "_blank" %></td>
  <td><%= link_to "#{ENV['ROOT_URL']}#{url_pair.shortened}", "#{ENV['ROOT_URL']}#{url_pair.shortened}", target: "_blank" %></td>
  <td><%= url_pair.visits_count %></td>
  <td><%= distance_of_time_in_words((url_pair.created_at - 14.days).beginning_of_day, url_pair.created_at) %></td>
</tr>